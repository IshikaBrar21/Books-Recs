    <script>
    document.addEventListener('DOMContentLoaded', function() {
        var myCarousel = document.querySelector('#myCarousel');
        var carousel = new bootstrap.Carousel(myCarousel, {
            touch: false
        });
        
    $("#beeImage").hover(
            function(event) {
                $("#buzzMessage").css({
                    display: 'block',
                    left: event.pageX + 'px',
                    top: event.pageY - 30 + 'px'
                });
            },
            function() {
                $("#buzzMessage").css({
                    display: 'none'
                });
            }
        );
        $("#beeImage").mousemove(function(event) {
            $("#buzzMessage").css({
                left: event.pageX + 'px',
                top: event.pageY + 20 + 'px' 
            });
        });
        $(".covers").click(function() {
        var slideTo = $(this).data('bs-slide-to');
        carousel.to(slideTo);
    });
    });
</script>
